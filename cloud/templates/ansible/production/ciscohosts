lab:
  children:
    rtr:
    srt:
  vars:
    ansible_user: root

rtr:
  hosts:
    rtr1lab:
      ansible_host: 192.168.0.1
      hostname:
        short: rtr1lab
        domain: example.com

srt:
  hosts:
    srt1lab:
      ansible_host: 192.168.0.2
      hostname:
        short: srt1lab
        domain: example.com

